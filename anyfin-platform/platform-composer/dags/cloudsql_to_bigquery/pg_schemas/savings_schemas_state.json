{
    "savings_customers": {
      "schema": {
        "id":"integer", 
        "created_at":"timestamp with time zone", 
        "updated_at":"timestamp with time zone", 
        "customer_id":"integer", 
        "has_external_account":"boolean"
      },
      "ts_column": "updated_at",
      "bq_partition_column": "created_at",
      "backfill_method": "beam_backfill"
    },
    "customer_triggers": {
      "schema": {
        "id":"integer", 
        "created_at":"timestamp with time zone", 
        "updated_at":"timestamp with time zone", 
